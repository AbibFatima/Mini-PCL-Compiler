    0 $accept: s $end

    1 s: IDF '{' VAR '{' DECLARATION '}' CODE '{' Instructions '}' '}'

    2 DECLARATION: DECLARATION_Variables ';' DECLARATION
    3            | DECLARATION_Struct ';' DECLARATION
    4            | DECLARATION_Struct_Var ';' DECLARATION
    5            | DECLARATION_Tableau ';' DECLARATION
    6            | DECLARATION_Const ';' DECLARATION
    7            | COMMENT DECLARATION
    8            | /* vide */

    9 DECLARATION_Variables: TYPE liste_idf

   10 liste_idf: IDF ',' liste_idf
   11          | IDF

   12 DECLARATION_Struct: STRUCT '{' declarStructFields '}' IDF

   13 $@1: /* vide */

   14 DECLARATION_Struct_Var: STRUCT IDF $@1 liste_idf

   15 DECLARATION_Const: CONST IDF AFF literal

   16 DECLARATION_Tableau: TYPE IDF '[' ENTIER ']'

   17 declarStructFields: TYPE IDF ',' declarStructFields
   18                   | /* vide */

   19 TYPE: INTEGER
   20     | FLOAT

   21 literal: ENTIER
   22        | REEL

   23 Instructions: Inst Instructions
   24             | /* vide */

   25 Inst: Inst_AFF ';'
   26     | Inst_IF
   27     | Inst_WHILE
   28     | Inst_FOR
   29     | COMMENT

   30 Inst_AFF: IDF AFF expression
   31         | IDF '[' ENTIER ']' AFF expression

   32 Inst_IF: B ELSE '{' Instructions '}'
   33        | B

   34 B: A '{' Instructions '}'

   35 A: IF '(' Expression_Condition ')'

   36 Inst_WHILE: WHILE '(' Expression_Condition ')' '{' Instructions '}'

   37 Inst_FOR: FOR '(' Expression_FOR ')' '{' Instructions '}'

   38 Expression_FOR: Init_FOR ':' Pas_FOR ':' Cond_arret

   39 Init_FOR: IDF ':' literal

   40 Pas_FOR: literal

   41 Cond_arret: IDF
   42           | literal

   43 Expression_Condition: Condition AND Expression_Condition
   44                     | Condition OR Expression_Condition
   45                     | Condition

   46 Condition: expression OP_expression expression
   47          | NOT expression

   48 OP_expression: GREATER
   49              | LESS
   50              | EQUAL
   51              | GREATEREQUAL
   52              | LESSEQUAL
   53              | NOTEQUAL

   54 expression: expression ADD expression
   55           | expression SUB expression
   56           | expression DIV expression
   57           | expression MUL expression
   58           | IDF
   59           | ENTIER
   60           | REEL
   61           | '(' expression ')'


